# ============================================
# Semantic-Sync Configuration
# ============================================
# Copy this file to .env and fill in your actual credentials

# ============================================
# SNOWFLAKE SETTINGS
# ============================================
SNOWFLAKE_ACCOUNT=your_account.region
SNOWFLAKE_USER=your_username
SNOWFLAKE_PASSWORD=your_password
SNOWFLAKE_WAREHOUSE=your_warehouse
SNOWFLAKE_DATABASE=your_database
SNOWFLAKE_SCHEMA=PUBLIC
SNOWFLAKE_ROLE=your_role
SNOWFLAKE_SEMANTIC_VIEW=your_semantic_view_name

# ============================================
# MICROSOFT FABRIC SETTINGS
# ============================================
FABRIC_TENANT_ID=your-azure-tenant-id
FABRIC_CLIENT_ID=your-azure-app-client-id
FABRIC_CLIENT_SECRET=your-azure-app-client-secret
FABRIC_WORKSPACE_ID=your-fabric-workspace-id
FABRIC_DATASET_ID=your-fabric-dataset-id

# ============================================
# SYNC SETTINGS (Optional)
# ============================================
# SYNC_BATCH_SIZE=100
# SYNC_TIMEOUT=300
# SYNC_MAX_RETRIES=3
# LOG_LEVEL=INFO
